    "# # Shake Drop____________________________________________________________________\n",
    "import numpy as np\n",
    "from scipy.stats import bernoulli\n",
    "\n",
    "from chainer import cuda\n",
    "from chainer import initializers\n",
    "from chainer import configuration\n",
    "from chainer import Variable\n",
    "import chainer.functions as F\n",
    "import chainer.links as L\n",
    "import chainer.distributions as D\n",
    "\n",
    "\n",
    "class ShakeDrop(chainer.function.Function):\n",
    "    \n",
    "    def __init__(self, pl):\n",
    "        self.pl = pl\n",
    "        return\n",
    "\n",
    "    def forward(self, x):\n",
    "        xp = cuda.get_array_module(x) # Get numpy(x=n) or cupy(x=c) array module\n",
    "        alpha = xp.ones(x.shape, dtype=x.dtype)\n",
    "        bl = xp.ones(x.shape, dtype=x.dtype)\n",
    "        bl = bl * bernoulli.pmf(1, self.pl) # bl value is 0 or 1\n",
    "        \n",
    "        alpha_shape = ([1] * len(alpha.shape))\n",
    "        alpha_shape[0] = -1\n",
    "        alpha[:] = ((xp.random.rand(len(alpha)) * 2.0) - 1.0).reshape(alpha_shape)\n",
    "\n",
    "        if not configuration.config.train: # otherwise(e.g. test)\n",
    "            alpha_ave = xp.average(alpha)\n",
    "            alpha[:] = alpha_ave\n",
    "\n",
    "        return (bl + alpha - bl*alpha) * x\n",
    "    \n",
    "    def backward(self, grad_outputs):\n",
    "        gx = grad_outputs\n",
    "        xp = cuda.get_array_module(gx)\n",
    "        beta = xp.ones(gx.shape, dtype=gx.dtype)\n",
    "        bl = xp.ones(gx.shape, dtype=gx.dtype)\n",
    "        bl = bl * bernoulli.pmf(1, self.pl) # bl value is 0 or 1\n",
    "\n",
    "        for i in range(len(beta)):\n",
    "            beta[i] = xp.random.rand() # beta range is [0, 1)\n",
    "        \n",
    "        return (bl + beta - bl*beta) * gx\n",
    "\n",
    "def shake_drop(x, pl, back=False):\n",
    "    if back:\n",
    "    return ShakeDrop(pl).backward(x)\n",
    "else:\n",
    "    return ShakeDrop(pl).forward(x)\n",
   ]
