    "# PyramidNet____________________________________________________________________\n",
    "# Network_______________________________________________________________________\n",
    "\n",
    "class BottleNeckA(chainer.Chain):\n",
    "    outchannel_ratio = 4\n",
    "\n",
    "    def __init__(self, in_size, ch, stride=2):\n",
    "        super(BottleNeckA, self).__init__()\n",
    "        initialW = initializers.HeNormal()\n",
    "\n",
    "        with self.init_scope():\n",
    "            self.bn1 = L.BatchNormalization(in_size)\n",
    "            self.conv1 = L.Convolution2D(in_size, ch, 1, stride, 0,\n",
    "                                         initialW=initialW, nobias=True)\n",
    "            self.bn2 = L.BatchNormalization(ch)\n",
    "            self.conv2 = L.Convolution2D(ch, ch * BottleNeckA.outchannel_ratio, 3, 1, 1,\n",
    "                                         initialW=initialW, nobias=True)\n",
    "            self.bn3 = L.BatchNormalization(ch * BottleNeckA.outchannel_ratio)\n",
    "            self.conv4 = L.Convolution2D(in_size, ch * BottleNeckA.outchannel_ratio, 1, stride, 0,\n",
    "                                         initialW=initialW, nobias=True)\n",
    "            self.bn4 = L.BatchNormalization(ch * BottleNeckA.outchannel_ratio)\n",
    "\n",
    "    def __call__(self, x):\n",
    "\n",
    "        h1 = self.conv1(self.bn1(x))\n",
    "        h1 = self.conv2(F.relu(self.bn2(h1)))\n",
    "        h1 = self.bn3(h1)\n",
    "\n",
    "        h2 = self.bn4(self.conv4(x))\n",
    "\n",
    "        batch_size = h1.shape[0]\n",
    "        residual_channel = h1.shape[1]\n",
    "        shortcut_channel = h2.shape[1]\n",
    "        featuremap_size = h1.shape[2:4]\n",
    "\n",
    "        if residual_channel != shortcut_channel:\n",
    "            xp = chainer.cuda.get_array_module(x)\n",
    "            pad = chainer.Variable(\n",
    "                xp.zeros((batch_size,\n",
    "                          residual_channel - shortcut_channel,\n",
    "                          featuremap_size[0],\n",
    "                          featuremap_size[1]), dtype=xp.float32))\n",
    "            return h1 + F.concat((h2, pad), axis=1)\n",
    "        else:\n",
    "            return h1 + h2\n",
    "\n",
    "\n",
    "class BottleNeckB(chainer.Chain):\n",
    "    outchannel_ratio = 4\n",
    "\n",
    "    def __init__(self, in_size, ch, depth, now_layer):\n",
    "        super(BottleNeckB, self).__init__()\n",
    "        initialW = initializers.HeNormal()\n",
    "        self.now_layer = now_layer\n",
    "        self.depth = depth\n",
    "\n",
    "        with self.init_scope():\n",
    "            self.bn1 = L.BatchNormalization(in_size)\n",
    "            self.conv1 = L.Convolution2D(in_size, ch, 1, 1, 0,\n",
    "                                         initialW=initialW, nobias=True)\n",
    "            self.bn2 = L.BatchNormalization(ch)\n",
    "            self.conv2 = L.Convolution2D(ch, ch, 3, 1, 1,\n",
    "                                         initialW=initialW, nobias=True)\n",
    "            self.bn3 = L.BatchNormalization(ch)\n",
    "            self.conv3 = L.Convolution2D(ch, ch * BottleNeckB.outchannel_ratio, 1, 1, 0,\n",
    "                                         initialW=initialW, nobias=True)\n",
    "            self.bn4 = L.BatchNormalization(ch * BottleNeckB.outchannel_ratio)\n",
    "\n",
    "    def forward(self, x):\n",
    "        pl = 1 - (self.now_layer/(2*self.depth))\n",
    "        \n",
    "        h = self.conv1(self.bn1(x))\n",
    "        h = self.conv2(F.relu(self.bn2(h)))\n",
    "        h = self.conv3(F.relu(self.bn3(h)))\n",
    "        h = self.bn4(h)\n",
    "        h = shake_drop(h, pl)\n",
    "\n",
    "        batch_size = h.shape[0]\n",
    "        residual_channel = h.shape[1]\n",
    "        shortcut_channel = x.shape[1]\n",
    "        featuremap_size = h.shape[2:4]\n",
    "\n",
    "        if residual_channel != shortcut_channel:\n",
    "            xp = chainer.cuda.get_array_module(x)\n",
    "            pad = chainer.Variable(\n",
    "                xp.zeros((batch_size,\n",
    "                          residual_channel - shortcut_channel,\n",
    "                          featuremap_size[0],\n",
    "                          featuremap_size[1]), dtype=xp.float32))\n",
    "            return h + F.concat((x, pad), axis=1)\n",
    "        else:\n",
    "            return h + x\n",
    "\n",
    "    def backward(self, x): # only revise shake_drop",
    "        pl = 1 - (self.now_layer/(2*self.depth))",
    "\n",
    "        h = self.conv1(self.bn1(x))",
    "        h = self.conv2(F.relu(self.bn2(h)))",
    "        h = self.conv3(F.relu(self.bn3(h)))",
    "        h = self.bn4(h)",
    "        h = shake_drop(h, pl, back=True)",
    "\n",
    "        batch_size = h.shape[0]",
    "        residual_channel = h.shape[1]",
    "        shortcut_channel = x.shape[1]",
    "        featuremap_size = h.shape[2:4]",
    "\n",
    "        if residual_channel != shortcut_channel:",
    "            xp = chainer.cuda.get_array_module(x)",
    "            pad = chainer.Variable(",
    "                xp.zeros((batch_size,",
    "                          residual_channel - shortcut_channel,",
    "                          featuremap_size[0],",
    "                          featuremap_size[1]), dtype=xp.float32))",
    "            return h + F.concat((x, pad), axis=1)",
    "        else:",
    "            return h + x",
    "\n",
    "class Block(chainer.Chain):\n",
    "\n",
    "    def __init__(self, depth, now_layer, block_depth, in_size, mid_size, addrate, stride=2):\n",
    "        self.in_size = in_size\n",
    "        self.mid_size = mid_size\n",
    "        self.depth = depth\n",
    "        self.now_layer = now_layer\n",
    "        self.block_depth = block_depth\n",
    "\n",
    "        super(Block, self).__init__()\n",
    "        with self.init_scope():\n",
    "            self.mid_size = self.mid_size + addrate\n",
    "\n",
    "            self.a = BottleNeckA(self.in_size, int(round(self.mid_size)), stride)\n",
    "            for i in range(1, self.block_depth):\n",
    "                temp_featuremap_dim = self.mid_size + addrate\n",
    "                block = BottleNeckB(int(round(self.mid_size)) * BottleNeckB.outchannel_ratio,\n",
    "                                    int(round(temp_featuremap_dim)), self.depth, \n",
    "                                    (self.now_layer+i*3+1))\n",
    "                setattr(self, 'b{}'.format(i), block)\n",
    "                self.mid_size = temp_featuremap_dim\n",
    "\n",
    "            self.last_size = int(round(self.mid_size)) * BottleNeckB.outchannel_ratio\n",
    "\n",
    "    def __call__(self, x):\n",
    "        h = self.a(x)\n",
    "        for i in range(1, self.block_depth):\n",
    "            h = self['b{}'.format(i)](h)\n",
    "\n",
    "        return h\n",
    "\n",
    "\n",
    "class PyramidNetLayers(chainer.Chain):\n",
    "\n",
    "    def __init__(self, depth, alpha, class_num):\n",
    "        super(PyramidNetLayers, self).__init__()\n",
    "        self.ch = 64\n",
    "        self.depth = depth\n",
    "        self.block_depth = int((depth - 2) / 9)\n",
    "        self.now_layer = self.block_depth*3\n",
    "        self.addrate = alpha / (3 * self.block_depth)\n",
    "        self.class_num = class_num\n",
    "\n",
    "        with self.init_scope():\n",
    "            self.conv1 = L.Convolution2D(\n",
    "                3, self.ch, 3, 1, 1, nobias=True, initialW=initializers.HeNormal())\n",
    "            self.bn1 = L.BatchNormalization(self.ch)\n",
    "            self.py1 = Block(self.depth, self.now_layer*0, self.block_depth, self.ch, self.ch, self.addrate, stride=1)\n",
    "            self.py2 = Block(self.depth, self.now_layer*1, self.block_depth, self.py1.last_size, self.py1.mid_size, self.addrate)\n",
    "            self.py3 = Block(self.depth, self.now_layer*2, self.block_depth, self.py2.last_size, self.py2.mid_size, self.addrate)\n",
    "            self.bn2 = L.BatchNormalization(self.py3.last_size)\n",
    "            self.fc = L.Linear(self.py3.last_size, self.class_num)\n",
    "\n",
    "    def __call__(self, x):\n",
    "        h = self.bn1(self.conv1(x))\n",
    "        h = F.max_pooling_2d(F.relu(h), 2, 2, 0)\n",
    "        h = self.py1(h)\n",
    "        h = self.py2(h)\n",
    "        h = self.py3(h)\n",
    "        h = F.average_pooling_2d(F.relu(self.bn2(h)), 2, 2, 0)\n",
    "        h = self.fc(h)\n",
    "\n",
    "        return h\n",
    "\n",
    "\n",
    "class PyramidNet101(PyramidNetLayers):\n",
    "\n",
    "    def __init__(self, class_num=None):\n",
    "        super(PyramidNet101, self).__init__(depth=101, alpha=250, class_num=None)\n",
    "\n",
    "\n",
    "class PyramidNet152(PyramidNetLayers):\n",
    "\n",
    "    def __init__(self, class_num=None):\n",
    "        super(PyramidNet152, self).__init__(depth=152, alpha=200, class_num=None)\n",
    "\n",
    "\n",
    "class PyramidNet200(PyramidNetLayers):\n",
    "\n",
    "    def __init__(self, class_num=None):\n",
    "        super(PyramidNet200, self).__init__(depth=200, alpha=300, class_num=None)"
